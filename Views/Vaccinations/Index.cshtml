        <td>
            @Html.DisplayFor(modelItem => item.Country.CountryName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Vaccine.VaccineName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstDose)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SecondDose)
        </td>
        <td>
